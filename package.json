{
    "name": "ertools",
    "version": "3.0.0",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "Tools for dealing with database applications specified by entity-relationship diagrams",
    "category": [ "Database" ],
    "dependencies": {
        "base"           : ">= 3.0.0, < 4.0.0",
        "abstract-curry" : ">= 3.0.0, < 4.0.0",
        "cdbi"           : ">= 3.0.0, < 4.0.0",
        "currypath"      : ">= 3.0.0, < 4.0.0",
        "directory"      : ">= 3.0.0, < 4.0.0",
        "filepath"       : ">= 3.0.0, < 4.0.0",
        "flatcurry"      : ">= 3.0.0, < 4.0.0",
        "graphviz"       : ">= 3.0.0, < 4.0.0",
        "io-extra"       : ">= 3.0.0, < 4.0.0",
        "keydb"          : ">= 3.0.0, < 4.0.0",
        "process"        : ">= 3.0.0, < 4.0.0",
        "setfunctions"   : ">= 3.0.0, < 4.0.0",
        "time"           : ">= 3.0.0, < 4.0.0",
        "wl-pprint"      : ">= 3.0.0, < 4.0.0",
        "xml"            : ">= 3.0.0, < 4.0.0"
    },
    "exportedModules": [ "Database.ERD.Goodies", "Database.ERD.Generic" ],
    "configModule": "ERToolsPackageConfig",
    "executable": {
        "name": "erd2curry",
        "main": "ERD2Curry"
    },
    "license": "BSD-3-Clause",
    "licenseFile": "LICENSE",
    "source": {
        "git": "https://git.ps.informatik.uni-kiel.de/curry-packages/ertools.git",
        "tag": "$version"
    }
}
