{
    "name": "ertools",
    "version": "2.0.0",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "Tools for dealing with database applications specified by entity-relationship diagrams",
    "category": [ "Database" ],
    "dependencies": {
        "abstract-curry": ">= 2.0.0",
        "flatcurry"     : ">= 2.0.0",
        "graphviz"      : ">= 2.0.0",
        "xml"           : ">= 2.0.0"
    },
    "compilerCompatibility": {
        "pakcs": ">= 2.0.0",
        "kics2": ">= 2.0.0"
    },
    "exportedModules": [ "Database.ERD.Goodies" ],
    "configModule": "ERToolsPackageConfig",
    "executable": {
        "name": "erd2curry",
        "main": "ERD2Curry"
    },
    "source": {
        "git": "https://git.ps.informatik.uni-kiel.de/curry-packages/ertools.git",
        "tag": "$version"
    }
}
